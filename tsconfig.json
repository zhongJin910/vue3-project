{
  "compilerOptions": {
    "sourceMap": true,
    "baseUrl": "./",
    "paths": { "@/*": ["src/*"] },
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modeules", "dist"],
  "include": ["src"]
}
